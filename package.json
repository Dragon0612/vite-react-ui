{
  "name": "vite-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:test": "vite --mode test",
    "dev:prod": "vite --mode production",
    "build": "vite build --mode production",
    "build:test": "vite build --mode test",
    "build:dev": "vite build --mode development",
    "build:analyze": "vite build --mode production && node scripts/analyze.js",
    "preview": "vite preview",
    "preview:test": "vite preview --port 4174",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "performance": "node scripts/performance.js",
    "performance:test": "node scripts/simple-performance-test.js",
    "build:full": "npm run build && npm run performance",
    "start": "npm run dev",
    "test": "npm run dev:test",
    "prod": "npm run dev:prod"
  },
  "dependencies": {
    "antd": "^5.26.7",
    "axios": "^1.11.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.7.1",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^24.1.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "consola": "^3.4.2",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "less": "^4.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "terser": "^5.43.1",
    "vite": "^7.0.4",
    "vite-plugin-chunk-split": "^0.5.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-imagemin": "0.6.1",
    "vite-plugin-purgecss": "^0.2.13"
  }
}
